<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".MyAccount">    <LinearLayout        android:layout_width="match_parent"        android:id="@+id/top"        android:layout_height="wrap_content"        android:background="@color/colorDark"        app:layout_constraintTop_toTopOf="parent">        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:padding="16dp"            android:onClick="toMainActivity"            android:tint="@color/colorLight"            android:src="@drawable/ic_arrow_back_black_24dp" />        <TextView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:text="Selamat Datang"            android:layout_marginTop="16dp"            android:layout_marginBottom="16dp"            android:gravity="center_vertical"            android:textColor="@color/colorLight"            android:textSize="20sp"            android:textStyle="bold" />    </LinearLayout>    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="@android:color/darker_gray"        app:layout_constraintTop_toBottomOf="@id/top" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_margin="16dp"        android:gravity="center"        app:layout_constraintBottom_toBottomOf="parent"        android:orientation="vertical"        app:layout_constraintTop_toBottomOf="@id/top">        <ImageView            android:layout_width="96dp"            android:layout_height="96dp"            android:layout_margin="16dp"            android:src="@drawable/ic_account_circle_black_24dp" />        <android.support.v7.widget.CardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="16dp"            android:layout_marginRight="16dp"            android:elevation="4dp">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <TextView                    android:id="@+id/webTokoText"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:background="@color/colorBlue"                    android:drawableRight="@drawable/ic_edit_black_24dp"                    android:drawablePadding="8dp"                    android:drawableTint="@color/colorLight"                    android:onClick="toUbahProfil"                    android:padding="8dp"                    android:text="Nurhadi Aldo"                    android:textColor="@color/colorLight"                    android:textSize="16sp"                    android:textStyle="bold" />                <TableLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="8dp">                    <TableRow                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:text="Bengkel:"                            android:textStyle="bold" />                        <TextView                            android:layout_width="0dp"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:layout_weight="1"                            android:text="PT. Maju Terus" />                    </TableRow>                    <TableRow                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:text="Alamat :"                            android:textStyle="bold" />                        <TextView                            android:layout_width="0dp"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:layout_weight="1"                            android:text="Jl. Sukamaju No.17 RT/RW 001/007 Maju Terus, Bandung 40117" />                    </TableRow>                    <TableRow                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:text="Kontak :"                            android:textStyle="bold" />                        <TextView                            android:layout_width="0dp"                            android:layout_height="wrap_content"                            android:layout_margin="4dp"                            android:layout_weight="1"                            android:text="081987456321" />                    </TableRow>                </TableLayout>            </LinearLayout>        </android.support.v7.widget.CardView>        <Button            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_margin="16dp"            android:background="#00000000"            android:onClick="toLoginActivity"            android:text="SIGN OUT"            android:textColor="@color/colorBlue"            android:textStyle="bold" />    </LinearLayout></android.support.constraint.ConstraintLayout>